{
  "name": "bg-tracking",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --names next,bot --prefix-colors cyan,yellow \"next dev -p 3001\" \"npx tsx src/bot/polling.ts\"",
    "dev:next": "next dev -p 3001",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:reset": "npx prisma migrate reset --force",
    "db:studio": "npx prisma studio",
    "bot": "npx tsx src/bot/polling.ts"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/client": "^7.4.0",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.97.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "grammy": "^1.40.0",
    "lucide-react": "^0.575.0",
    "next": "16.1.6",
    "openai": "^6.22.0",
    "papaparse": "^5.5.3",
    "pdf-parse": "^2.4.5",
    "pg": "^8.13.0",
    "prisma": "^7.4.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "recharts": "^3.7.0",
    "sonner": "^2.0.7",
    "xlsx": "^0.18.5",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/papaparse": "^5.5.2",
    "@types/pdf-parse": "^1.1.5",
    "@types/pg": "^8.11.10",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "concurrently": "^9.2.1",
    "dotenv": "^17.3.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
